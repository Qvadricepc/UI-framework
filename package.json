{
  "name": "@ondefy/ondefy-ui",
  "version": "0.0.0-alpha.64",
  "engines": {
    "node": "^16.0.0"
  },
  "declaration": true,
  "typings": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/ondefy/ondefy-ui",
  "bugs": {
    "url": "https://github.com/ondefy/ondefy-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ondefy/ondefy-ui.git"
  },
  "license": "ISC",
  "author": "Darmikon",
  "files": [
    "dist"
  ],
  "scripts": {
    "#postversion": "git push && git push --tags",
    "#preversion": "npm run lint",
    "#version": "npm run format && git add -A src",
    "#changelog": "conventional-changelog --preset angular -i CHANGELOG.md -s -r 1",
    "#changelog:help": "conventional-changelog --help",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "prepublishOnly": "npm run build",
    "tslint": "tsc --noEmit",
    "stylelint": "stylelint \"**/*.{ts,tsx,js,jsx}\"",
    "test": "ENV=test node ./node_modules/jest/bin/jest.js --runInBand --silent --passWithNoTests",
    "qa": "npm run lint && npm run tslint && npm run stylelint && npm run test",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "start": "npm run storybook",
    "build-storybook": "build-storybook",
    "deploy": "npm run build-storybook && gh-pages -d storybook-static && rm -rf storybook-static",
    "_icons-lint": "eslint src/common/components/o-icon/*.icons.ts src/common/components/o-svg-icon/*.icons.ts --fix",
    "icons": "node public/assets/fonts/icomoon.generator.js & node public/assets/svg/icomoon.generator.js & npm run _icons-lint && git add .",
    "prepare": "husky install",
    "build": "rollup -c"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.6",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react-cookie": "^4.1.1",
    "react-use": "^17.4.0",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@svgr/rollup": "^6.5.1",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^13.4.0",
    "@types/dompurify": "^2.4.0",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.15",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-storybook": "^0.6.8",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.0",
    "rollup": "^2.66.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^29.0.3",
    "typescript": "*"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "styled-system": "^5.1.5"
  }
}
